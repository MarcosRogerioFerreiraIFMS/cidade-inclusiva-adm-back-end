    "studio": "prisma studio",
    "push": "prisma db push",
    "pull": "prisma db pull",
    "status": "prisma migrate status",
    "first": "pnpm install && pnpm migrate && pnpm dev",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "build:noEmit": "tsc --noEmit",
    "check": "tsx ./scripts/check.ts",
    "clean": "tsx ./scripts/clean.ts",
    "generate-jwt-secret": "tsx ./scripts/generate-jwt-secret.ts",
    "validate-env": "tsx ./scripts/validate-env.ts",
    "check-node-version": "tsx ./scripts/check-node-version.ts"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  },
